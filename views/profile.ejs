                        </svg><b>Edit Property</b></button>
                      </div>
                    </div>
                  </div>
                  <% } %>
               <% }) %>
            <% } %>
            </div>
            <div class="tabC"><% if(user.property.length > 0){ %>
              <% user.property.forEach((property)=>{ %>
                <% if(property.sold == "yes"){ %>
                <div class="resultedDiv flex border">
                  <div class="swiper mySwiper">
                    <div class="swiper-wrapper">
                      <% property.media.forEach((media)=>{ %>
                        <% if(media.includes("jpg") || media.includes("jpeg") || media.includes("png")){ %>
